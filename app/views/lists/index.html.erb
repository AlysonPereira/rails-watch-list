<div class="container"><h1 class="text-white text-center">My Lists</h1></div>
<div class="container">
  <div class="cards">
    <% @lists.each do |list| %>
        <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= image_path 'background.jpg'%>)">
          <%= link_to list.name, list_path(list), class: 'text-decoration-none' %>
        </div>
    <% end %>
  </div>
</div>

<div class="container"></div>
